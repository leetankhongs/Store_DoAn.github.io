                            <a href="/product_details/{{this.id}}"><img src="{{this.Image}}" alt=""/></a>
                            <div class="caption">
                            <h5>{{this.SimpleDetail}}</h5>
                                <h4 style="text-align:center"> <a class="btn" href="#">Thêm vào giỏ<i class="icon-shopping-cart"></i></a> <a class="btn btn-primary" href="#">{{this.Cost}}</a></h4>
                            </div>
                        </div>
                    </li>
					{{/each}}
				  </ul>
			<hr class="soft"/>
			</div>
		</div>
				<br class="clr">
					 </div>
